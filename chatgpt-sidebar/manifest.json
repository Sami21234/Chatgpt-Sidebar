{
    "manifest_version": 3,
    "name": "ChatGPT Question Sidebar" ,
    "version": "1.0" ,
    "description": "Right-side panel listing your questions in the current ChatGPT chat. ",
    "permissions": ["scripting", "activeTab"],
    "content_scripts": [
        {
            "matches":
            ["https://chat.openai.com/*", "https://Chatgpt.com/*"],
            "js": ["content.js"]
        }
    ]
}

